{
  "id": "default",
  "name": "Default Dark",
  "version": "1.0.0",
  "author": "PwnThemAll Team",
  "description": "Clean dark theme for CTF platforms",
  "colors": {
    "primary": "#3b82f6",
    "primaryHover": "#2563eb",
    "secondary": "#8b5cf6",
    "secondaryHover": "#7c3aed",
    "accent": "#06b6d4",
    "background": "#0a0a0a",
    "backgroundAlt": "#111111",
    "surface": "#1a1a1a",
    "surfaceHover": "#252525",
    "surfaceActive": "#2a2a2a",
    "text": "#ffffff",
    "textMuted": "#a0a0a0",
    "textInverted": "#000000",
    "border": "#333333",
    "borderHover": "#444444",
    "success": "#22c55e",
    "successHover": "#16a34a",
    "danger": "#ef4444",
    "dangerHover": "#dc2626",
    "warning": "#f59e0b",
    "warningHover": "#d97706",
    "info": "#3b82f6",
    "infoHover": "#2563eb",
    "solved": "#22c55e",
    "locked": "#6b7280",
    "shadow": "#000000",
    "shadowGlow": "#3b82f6"
  },
  "typography": {
    "fontFamily": {
      "heading": "'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
      "body": "'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
      "mono": "'JetBrains Mono', 'Fira Code', 'Courier New', monospace"
    },
    "scale": "normal",
    "fontWeights": {
      "normal": 400,
      "medium": 500,
      "semibold": 600,
      "bold": 700
    },
    "lineHeights": {
      "tight": 1.25,
      "normal": 1.5,
      "relaxed": 1.75
    }
  },
  "spacing": {
    "scale": 1.0,
    "containerPadding": "1rem",
    "sectionGap": "2rem"
  },
  "layout": {
    "cardStyle": "elevated",
    "cardCorners": "rounded",
    "headerStyle": "normal",
    "tableStyle": "default",
    "badgeShape": "rounded",
    "buttonStyle": "rounded"
  },
  "effects": {
    "animations": true,
    "glowEffects": false,
    "hoverScale": true,
    "transitionDuration": 200,
    "backdropBlur": false
  },
  "assets": {}
}
